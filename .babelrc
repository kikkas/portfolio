{
  "presets": [
    ["env", {
      "modules": false
    }]
  ],
  "plugins": [
    ["transform-imports", {
      "lodash": {
        "transform": "lodash/${member}",
        "preventFullImport": false
      }
    }],
    ["transform-react-jsx", { "pragma":"h" }],
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}
