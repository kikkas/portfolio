{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "compat"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 0,
    "no-unused-vars": ["warn", { "varsIgnorePattern": "h" }],
    "no-extra-semi": 1,
    "compat/compat": "error"
  }
}
